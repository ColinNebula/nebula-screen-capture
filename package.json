{
  "name": "nebula-screen-capture",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://ColinNebula.github.io/nebula-screen-capture",
  "description": "Professional screen recording made simple - A modern, feature-rich screen recording PWA",
  "author": "Nebula 3D Dev",
  "license": "MIT",
  "dependencies": {
    "@paypal/react-paypal-js": "8.9.2",
    "@stripe/react-stripe-js": "5.2.0",
    "@stripe/stripe-js": "8.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "clean": "rmdir /s /q build 2>nul & rmdir /s /q node_modules 2>nul",
    "clean:cache": "rmdir /s /q node_modules\\.cache 2>nul"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  }
}
